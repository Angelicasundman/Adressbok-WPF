<Application x:Class="Adressbok.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Adressbok"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
<!--Kopplar så vi kan använda oss av våra olika bibliotek för styles-->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Buttons.xaml"/>
                <ResourceDictionary Source="Resources/Labels.xaml"/>
                <ResourceDictionary Source="Resources/ListView.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
